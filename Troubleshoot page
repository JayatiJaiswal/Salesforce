    <script>   
    var upload3;
    (function(){
        $('#cancel_io').removeClass('collapsed');
        $("#training_").click(function(){
         $(this).css("background-color","rgb(0, 138, 203)");
        })
        $("#video").click(function(){
         $(this).css("background-color","#75bfe2");
        })      
        $("#video").hover(function(){
           $(this).css("background-color","#75bfe2");
                }); 
        
 
       $('.hidden-div').addClass('collapsed');
       $(".ttry").addClass('collapsed');       
       $('.list-group-item').on('click',function(evt){
        $('.ttry').hide(); 
        $('#videodiv').hide();  
        var hideElement = evt.target.getAttribute('data-hide').split(','),
            showElement = evt.target.getAttribute('data-show').split(','),i;
        for(i=0;i<= hideElement.length;i++){
           $('#'+hideElement[i]).hide();
        }
        for(i=0;i<= showElement.length;i++){
          $('#'+showElement[i]).show();
        }
        $('.list-group-item.active').removeClass('active');
        $(this).addClass('active');        
        $('html, body').animate({
            scrollTop: "0px"
        }, 800);
       });           
        $('.list-group-item1').on('click',function(evt){
        $('.ttry').hide();  
        $('#videodiv').hide();  
        var hideElement = evt.target.getAttribute('data-hide').split(','),
            showElement = evt.target.getAttribute('data-show').split(','),i;
        for(i=0;i<= hideElement.length;i++){
           $('#'+hideElement[i]).hide();
        }
        for(i=0;i<= showElement.length;i++){
          $('#'+showElement[i]).show();
        }
        $('.list-group-item1.active').removeClass('active');
        $(this).addClass('active');    
        $('html, body').animate({
            scrollTop: "0px"
        }, 800);
       });   
      
       $(window).ready(function(){
         var upload = upload = "r_io,generate_no_io,generate_io,submit_for_app,amount_edit_io,unlock_io,cancel_io,map_io,unmap_io,upload_io,prepay_io,agency_commission,cpl_io,conversion_io,other_IO",i;
         if(upload){
           $('.hidden-div').hide();
           $('#videodiv').hide();   
           var upload1 = upload.split(",");
           for(i=0;i<=upload1.length;i++){
                $('#'+upload1[i]).show();
           }        }
        else{
         upload = "r_io,generate_no_io,generate_io,submit_for_app,amount_edit_io,unlock_io,cancel_io,map_io,unmap_io,upload_io,prepay_io,agency_commission,cpl_io,conversion_io,other_IO";
        }              
        if(upload){
           $('.hidden-div').hide();
           $('#videodiv').hide();   
           var upload1 = upload.split(",");
           for(i=0;i<=upload1.length;i++){
                $('#'+upload1[i]).show();
           }    }          });          
        
        if(upload3=='error')
       { $('.videodiv').hide();
       }
   
        }());                     
        
        $(".ttry").on('click', function() {
            $(this).children("div").slideToggle();
            $(this).toggleClass("highlight");
            return false;
        }); 
 
        
    </script>
</apex:form>
